                                android:layout_height="match_parent"
                                android:text="@string/Q_j0401m" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="@string/j0401m" />

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/j0401mcheck"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:tag="0">

                            <CheckBox
                                android:id="@+id/j0401ma"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/j0401ma" />

                            <CheckBox
                                android:id="@+id/j0401mb"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/j0401mb" />

                            <CheckBox
                                android:id="@+id/j0401mc"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/j0401mc" />

                            <CheckBox
                                android:id="@+id/j0401md"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/j0401md" />

                            <CheckBox
                                android:id="@+id/j0401me"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@string/j0401me" />

                            <CheckBox
                                android:id="@+id/j0401mf"
